@misc{lemire2014,
   author = {Kaser, Owen and Lemire, Daniel},
   title = {Compressed bitmap indexes: beyond unions and intersections},
   volume = {1402},
   pages = {4466},
   note = {Accepted for publication in Software: Practice and Experience on August 14th 2014. Note that arXiv:1402.4073 [cs:DB] is a companion to this paper; while they share some text, each contains many results not in the other},
   abstract = {Compressed bitmap indexes are used to speed up simple aggregate queries in databases. Indeed, set operations like intersections, unions and complements can be represented as logical operations (AND,OR,NOT) that are ideally suited for bitmaps. However, it is less obvious how to apply bitmaps to more advanced queries. For example, we might seek products in a store that meet some, but maybe not all, criteria. Such threshold queries generalize intersections and unions; they are often used in information-retrieval and data-mining applications. We introduce new algorithms that are sometimes three orders of magnitude faster than a naive approach. Our work shows that bitmap indexes are more broadly applicable than is commonly believed.},
   keywords = {Computer Science - Databases
Computer Science - Data Structures and Algorithms},
   url = {http://adsabs.harvard.edu/abs/2014arXiv1402.4466K},
   year = {2014},
   type = {Electronic Article}
}

